<navigationBar
  id="navigationBar"
  arrow="{{ menuArrow }}"
  bind:arrowClick="getData"
  placeholderBg=""
  color="{{ color }}"
  title="{{ className }}"
  back="{{ true }}"
  background="{{ barBg }}"
  fixed="{{ fixed }}"
  backConfirm="{{ backConfirm }}"
  backStyle="{{ backStyle }}"
  backEvent="{{ backEvent }}"
  bind:back="backEvent"
  backHomeEvent="{{ backHomeEvent }}"
  bind:backHome="backHomeEvent"
></navigationBar>

<view class="introduction"> 当前就诊人：{{ username }}</view>
<view class="choContain">
  <view class="choice float" bindtap="assess" data-url="">医院评价</view>
  <view class="choice float" bindtap="assess" data-url="">医生评价</view>
</view>
