<!--pages/registor/registor.wxml-->
<navigationBar
  id="navigationBar"
  arrow="{{ menuArrow }}"
  bind:arrowClick="getData"
  placeholderBg=""
  color="{{ color }}"
  title="{{ className }}"
  back="{{ true }}"
  background="{{ barBg }}"
  fixed="{{ fixed }}"
  backConfirm="{{ backConfirm }}"
  backStyle="{{ backStyle }}"
  backEvent="{{ backEvent }}"
  bind:back="backEvent"
  backHomeEvent="{{ backHomeEvent }}"
  bind:backHome="backHomeEvent"
></navigationBar>
<view class="container">
  <view class="registorBox">
    <text>\n</text>
    <view class="registorText">
      {{registorData}} {{subject}} {{doctorName}}医生
    </view>
    <view class="registorText">{{timeInterval}}</view>
  </view>
</view>
